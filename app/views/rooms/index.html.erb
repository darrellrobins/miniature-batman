<% @rooms.each do | room | %>
<section>
	<h2><%= link_to room.name, room_path(room.id) %></h2>
	<p><span class='fieldlabel'>Room rate:</span>
		<span class="fieldValue"><%= number_to_currency room.price_in_pence %></span>
	</p>
</section>
<% end %>
<%= link_to "New", new_room_path, class: 'button' %>
